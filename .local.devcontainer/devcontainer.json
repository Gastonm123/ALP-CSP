{
  "dockerComposeFile": "compose.yml",
  "service": "haskell-dev",
  "workspaceFolder": "/workspace",

  "customizations": {
    "vscode": {
      "extensions": [
        "haskell.haskell",
        "hoovercj.haskell-linter"
      ],
      "settings": {
        "haskell.hlint.executablePath": "hlint",
        "haskell.manageHLS": "PATH",
        "haskell.logFile": "",
        "haskell.checkProject": false,
        "haskell.hlint.ignore": [
          "Redundant bracket"
        ]
      }
    }
  }
}